window.onscroll = function() { scrollFunction()} ;	// When the user scrolls down 50px from the top of the document, show the button
function scrollFunction()
{
 if (document.body.scrollTop > 50 || document.documentElement.scrollTop > 50)	{ document.getElementById("TerugNaarBoven").style.display = "block"; }
 else										{ document.getElementById("TerugNaarBoven").style.display = "none"; }
}
</script>
