<script>
$('#ToonMeer').hide( );
if ( $('#LangereOmschrijving').prop('scrollHeight') > $('#LangereOmschrijving').innerHeight() )
{
 $('#ToonMeer').show( );
}

function myFunctionToonMeer()
{
 var element = document.getElementById("LangereOmschrijving");
 element.classList.toggle("LeesMeer");
 $("#ToonMeer").hide();
 $("#ToonMinder").show();
}

function myFunctionToonMinder()
{
 var element = document.getElementById("LangereOmschrijving");
 element.classList.toggle("LeesMeer");
 window.scrollTo(0,350);
 $("#ToonMeer").show();
 $("#ToonMinder").hide();
}
</script><script>
$("a[href='#top']").click(function() { $("html, body").animate({ scrollTop: 0 }, "slow"); return false; });
</script>
